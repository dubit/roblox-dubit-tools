{
	"name": "environment-tool",
	"emitLegacyScripts": false,
	"tree": {
		"$className": "DataModel",

		"Players": {
			"$className": "Players",
			"$properties": {
				"CharacterAutoLoads": false
			}
		},

		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"$ignoreUnknownInstances": true,

			"Packages": {
				"$className": "Folder",
				"$path": "Packages",

				"EnvironmentTool": {
					"$path": "Source"
				}
			},

			"DevPackages": {
				"$className": "Folder",
				"$path": "DevPackages"
			},

			"Tests": {
				"$className": "Folder",
				"$path": "Tests"
			}
		}
	}
}