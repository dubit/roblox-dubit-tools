{
	"name": "simple-store",
	"tree": {
		"$className": "DataModel",

		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"$ignoreUnknownInstances": true,

			"Packages": {
				"$className": "Folder",
				"$path": "Packages",

				"SimpleStore": {
					"$path": "Source"
				}
			},

			"DevPackages": {
				"$className": "Folder",
				"$path": "DevPackages"
			}
		},

		"ServerScriptService": {
			"$className": "ServerScriptService",
			"$ignoreUnknownInstances": true,
			"$path": "Tests/Source"
		}
	}
}