{
	"name": "ugc-toolkit",
	"emitLegacyScripts": false,
	"tree": {
		"$className": "DataModel",
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"$ignoreUnknownInstances": true,
			"Packages": {
				"$className": "Folder",
				"$path": "Packages",
				"UgcToolkit": {
					"$path": "Source"
				}
			},
			"DevPackages": {
				"$className": "Folder",
				"$path": "DevPackages"
			}
		},
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"$ignoreUnknownInstances": true,
			"$path": "Tests/Source"
		}
	}
}